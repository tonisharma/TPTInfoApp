<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TPTInfoApp.Views.TunniplaanView"
             xmlns:viewModels="clr-namespace:TPTInfoApp.ViewModels"
             Title="{Binding Title}">

    <ContentPage.BindingContext>
        <viewModels:Tunniplaan/>
    </ContentPage.BindingContext>

    <ContentPage.Content>
        <StackLayout>
            <WebView Source="https://tpt.siseveeb.ee/veebivormid/tunniplaan" WidthRequest="500" HeightRequest="800"/>
        </StackLayout>
    </ContentPage.Content>

</ContentPage>